<?xml version="1.0" encoding="utf-8"?>
<com.wali.live.livesdk.live.view.MoveViewGroup xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/applause"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:src="@drawable/live_anchor_yinxiao_icon_1" />

    <ImageView
        android:id="@+id/clown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:src="@drawable/live_anchor_yinxiao_icon_2" />

    <ImageView
        android:id="@+id/cheers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:src="@drawable/live_anchor_yinxiao_icon_3" />

    <ImageView
        android:id="@+id/smile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:src="@drawable/live_anchor_yinxiao_icon_4" />

    <ImageView
        android:id="@+id/laugh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:src="@drawable/live_anchor_yinxiao_icon_5" />

    <ImageView
        android:id="@+id/close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:src="@drawable/live_anchor_yinxiao_icon_close" />

</com.wali.live.livesdk.live.view.MoveViewGroup>