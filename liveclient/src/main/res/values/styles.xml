<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyCheckBoxRight" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:drawableRight">@drawable/checkbox</item>
    </style>
    <style name="progressLoading">
        <item name="android:minHeight">16.67dip</item>
        <item name="android:maxHeight">16.67dip</item>
        <item name="android:minWidth">16.67dip</item>
        <item name="android:maxWidth">16.67dip</item>
        <item name="android:indeterminateOnly">true</item>
        <item name="android:progressDrawable">@drawable/progress_loading</item>
        <item name="android:indeterminateDrawable">@drawable/progress_loading</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">720</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
</resources>