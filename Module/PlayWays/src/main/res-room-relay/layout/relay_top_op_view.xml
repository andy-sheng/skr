<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="android.widget.RelativeLayout">

    <ImageView
        android:id="@+id/feed_back_iv"
        android:layout_width="@dimen/view_41_dp"
        android:layout_height="@dimen/view_26_dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/view_16_dp"
        android:background="@drawable/race_feed_back_icon" />

    <ImageView
        android:id="@+id/game_rule_iv"
        android:layout_width="@dimen/view_41_dp"
        android:layout_height="@dimen/view_26_dp"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/feed_back_iv"
        android:background="@drawable/race_rule_icon" />

    <ImageView
        android:id="@+id/iv_voice_setting"
        android:layout_width="@dimen/view_40_dp"
        android:layout_height="@dimen/view_26_dp"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/view_10_dp"
        android:layout_toLeftOf="@id/game_rule_iv"
        android:background="@drawable/xkf_yinyue" />

</merge>