<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="android.widget.RelativeLayout">

    <com.module.playways.grab.room.view.normal.view.SelfSingLyricView
        android:id="@+id/pk_self_sing_lyric_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.module.playways.grab.room.view.pk.view.PKSingCardView
        android:id="@+id/pk_sing_card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.module.playways.grab.room.view.common.SingCountDownView
        android:id="@+id/sing_count_down_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/view_10_dp"
        android:layout_marginTop="@dimen/view_98_dp" />

</merge>