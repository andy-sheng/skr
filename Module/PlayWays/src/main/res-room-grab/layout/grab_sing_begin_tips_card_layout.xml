<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="android.widget.RelativeLayout">

    <com.module.playways.grab.room.view.normal.NormalSingBeginTipsCardView
        android:id="@+id/normla_sing_beign"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/view_175_dp"
        android:visibility="gone" />

    <com.module.playways.grab.room.view.chorus.ChorusSingBeginTipsCardView
