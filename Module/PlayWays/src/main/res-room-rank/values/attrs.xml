<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="rankLED">
        <!--位置-->
        <attr name="position" format="enum">
            <enum name="left" value="0" />
            <enum name="mid" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="arcProgress">
        <!--位置-->
        <attr name="linesnum" format="integer" />
    </declare-styleable>

    <!--RankTopContainerView2的配置-->
    <declare-styleable name="topContainer">
        <!--模式-->
        <attr name="mode" format="enum">
            <enum name="rank" value="0" />
            <enum name="audition" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="gameType">
        <!--位置-->
        <attr name="type" format="enum">
            <enum name="voice" value="1" />
            <enum name="rank" value="4" />
            <enum name="grab" value="5" />
            <enum name="race" value="6" />
            <enum name="mic" value="7" />
            <enum name="relay" value="8" />
            <enum name="party" value="9" />
            <enum name="battle" value="10" />
        </attr>
    </declare-styleable>

</resources>