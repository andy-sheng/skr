<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:levelView="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_act_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_trans_80">

    <ImageView
        android:id="@+id/background_iv"
        android:layout_width="@dimen/view_290_dp"
        android:layout_height="@dimen/view_290_dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/view_122_dp"
        android:background="@drawable/zhanji_win_guangquan" />

    <com.opensource.svgaplayer.SVGAImageView
        android:id="@+id/result_svga"
        android:layout_width="@dimen/view_250_dp"
        android:layout_height="@dimen/view_230_dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/view_142_dp"
        android:visibility="gone"
        app:antiAlias="true"
        app:autoPlay="false" />

    <com.zq.level.view.NormalLevelView
        android:id="@+id/level_view"
        android:layout_width="@dimen/view_198_dp"
        android:layout_height="@dimen/view_218_dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/view_160_dp"
        android:visibility="gone"
        levelView:largeStar="@dimen/view_22_dp"
        levelView:mainHeight="@dimen/view_172_dp"
        levelView:mainWidth="@dimen/view_198_dp"
        levelView:normalStar="@dimen/view_22_dp"
        levelView:starDiffH="@dimen/view_12_dp"
        levelView:starTotalWidth="@dimen/view_182_dp"
        levelView:subBottom="10dp"
        levelView:subHeight="@dimen/view_24_dp"
        levelView:subWidth="@dimen/view_44_dp"
        levelView:txtColor="#0C2275"></com.zq.level.view.NormalLevelView>

    <com.module.playways.rank.room.view.RecordCircleView
        android:id="@+id/record_circle_view"
        android:layout_width="@dimen/view_60_dp"
        android:layout_height="@dimen/view_90_dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/view_420_dp"
        android:visibility="gone" />


</RelativeLayout>