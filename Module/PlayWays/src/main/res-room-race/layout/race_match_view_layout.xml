<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="android.support.constraint.ConstraintLayout">

    <ImageView
        android:id="@+id/match_status_iv"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/view_38_dp"
        android:background="@drawable/race_matching_icon"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/container"
        android:layout_width="@dimen/view_337_dp"
        android:layout_height="@dimen/view_255_dp"
        android:layout_marginTop="@dimen/view_4_dp"
        android:background="@drawable/race_match_view_bg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/match_status_iv" />

    <com.module.playways.race.room.view.matchview.RaceMatchItemView
        android:id="@+id/left_view"
        android:layout_width="@dimen/view_85_dp"
        android:layout_height="@dimen/view_100_dp"
        android:layout_marginLeft="@dimen/view_50_dp"
        android:layout_marginTop="@dimen/view_85_dp"
        app:layout_constraintLeft_toLeftOf="@id/container"
        app:layout_constraintTop_toBottomOf="@id/match_status_iv" />

    <com.module.playways.race.room.view.matchview.RaceMatchItemView
        android:id="@+id/right_view"
        android:layout_width="@dimen/view_85_dp"
        android:layout_height="@dimen/view_100_dp"
        android:layout_marginTop="@dimen/view_85_dp"
        android:layout_marginRight="@dimen/view_70_dp"
        app:layout_constraintRight_toRightOf="@id/container"
        app:layout_constraintTop_toBottomOf="@id/match_status_iv" />

    <ImageView
        android:id="@+id/deng_view"
        android:layout_width="@dimen/view_318_dp"
        android:layout_height="@dimen/view_242_dp"
        android:layout_marginRight="@dimen/view_18_dp"
        android:background="@drawable/race_match_view_deng1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/match_status_iv" />

    <TextView
        android:id="@+id/content_tv"
        android:layout_width="@dimen/view_240_dp"
        android:layout_height="@dimen/view_36_dp"
        android:layout_marginTop="@dimen/view_22_dp"
        android:layout_marginRight="@dimen/view_15_dp"
        android:gravity="center"
        android:textColor="#ffffdd6d"
        android:textSize="@dimen/textsize_20_dp"
        app:layout_constraintLeft_toLeftOf="@id/container"
        app:layout_constraintRight_toRightOf="@id/container"
        app:layout_constraintTop_toTopOf="@id/container" />

    <ImageView
        android:id="@+id/result_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@id/content_tv"
        app:layout_constraintLeft_toLeftOf="@id/content_tv"
        app:layout_constraintRight_toRightOf="@id/content_tv"
        app:layout_constraintTop_toTopOf="@id/content_tv" />

</merge>