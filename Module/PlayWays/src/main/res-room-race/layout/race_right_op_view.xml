<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:parentTag="android.support.constraint.ConstraintLayout">

    <com.common.view.ex.ExImageView
        android:id="@+id/vote_iv"
        android:layout_width="@dimen/view_95_dp"
        android:layout_height="@dimen/view_48_dp"
        app:bl_unSelected_drawable="@drawable/race_vote_normal_icon"
        app:bl_selected_drawable="@drawable/race_vote_selected_icon"/>

    <com.common.view.ex.ExImageView
        android:id="@+id/give_up_iv"
        android:layout_width="@dimen/view_95_dp"
        android:layout_height="@dimen/view_48_dp"
        app:bl_unSelected_drawable="@drawable/race_give_up_normal_icon"
        app:bl_selected_drawable="@drawable/race_give_up_selected_icon"
        android:visibility="gone" />

</merge>