<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:parentTag="android.support.constraint.ConstraintLayout">

    <ViewStub
        android:id="@+id/normal_self_sing_card_view_stub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:inflatedId="@+id/normal_self_sing_card_view"
        android:layout="@layout/grab_normal_self_sing_card_stub_layout"
        app:layout_constraintTop_toTopOf="parent" />

    <ViewStub
        android:id="@+id/chorus_self_sing_card_view_stub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/chorus_self_sing_card_view"
        android:layout="@layout/grab_chorus_self_sing_card_stub_layout"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent" />

    <ViewStub
        android:id="@+id/pk_self_sing_card_view_stub"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/pk_self_sing_card_view"
        android:layout="@layout/grab_pk_self_sing_card_stub_layout"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent" />

</merge>