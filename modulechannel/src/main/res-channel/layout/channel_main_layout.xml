<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:titlebar="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.common.view.titlebar.CommonTitleBar
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        titlebar:centerText="ChannelSdkListActivity"
        titlebar:centerType="textView" />

    <com.common.image.fresco.BaseImageView
        android:id="@+id/login_btn"
        android:layout_width="@dimen/public_view_dimen_200"
        android:layout_height="@dimen/public_view_dimen_200"
        android:layout_centerInParent="true"
        android:background="@color/public_black" />

</RelativeLayout>