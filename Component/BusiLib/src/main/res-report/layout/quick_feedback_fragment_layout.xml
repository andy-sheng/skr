<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <!--<View-->
    <!--android:id="@+id/place_holder_view"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="0dp"-->
    <!--android:background="@color/transparent"-->
    <!--android:visibility="gone" />-->

    <com.common.view.ex.ExRelativeLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/view_12_dp"
        android:layout_marginRight="@dimen/view_12_dp"
        android:paddingBottom="@dimen/view_24_dp"
        app:bl_corners_radius="@dimen/view_16_dp"
        app:bl_shape="rectangle"
        app:bl_solid_color="#D0EFFF">

        <com.zq.report.view.FeedbackView
            android:id="@+id/feed_back_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></com.zq.report.view.FeedbackView>

    </com.common.view.ex.ExRelativeLayout>

    <View
        android:id="@+id/place_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@color/transparent" />

    <ProgressBar
        android:id="@+id/upload_progress_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:visibility="gone" />

</RelativeLayout>