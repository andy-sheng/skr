<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--NormalLevelView的属性-->
    <declare-styleable name="levelView">
        <!--主段位宽度-->
        <attr name="mainWidth" format="dimension" />
        <!--主段位高度-->
        <attr name="mainHeight" format="dimension" />
        <!--子段位宽度-->
        <attr name="subWidth" format="dimension" />
        <!--子段位高度-->
        <attr name="subHeight" format="dimension" />
        <!--正常星星的大小-->
        <attr name="normalStar" format="dimension" />
        <!--大星星的大小-->
        <attr name="largeStar" format="dimension" />
        <!--相邻两颗星星高度差-->
        <attr name="starDiffH" format="dimension" />
        <!--所有星星高度总长度-->
        <attr name="starTotalWidth" format="dimension" />
        <!--横向星星文本的颜色-->
        <attr name="textColor" format="color|reference" />
    </declare-styleable>
</resources>
