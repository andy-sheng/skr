<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/level_iv"
        android:layout_width="@dimen/view_99_dp"
        android:layout_height="@dimen/view_86_dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/sub_leve_iv"
        android:layout_width="@dimen/view_15_dp"
        android:layout_height="@dimen/view_15_dp"
        android:layout_alignBottom="@+id/level_iv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/view_70_dp" />

    <com.common.view.ex.ExTextView
        android:id="@+id/star_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level_iv"
        android:layout_centerHorizontal="true"
        android:textColor="#FFED61"
        android:textSize="@dimen/textsize_16_dp"
        android:visibility="gone" />

</merge>