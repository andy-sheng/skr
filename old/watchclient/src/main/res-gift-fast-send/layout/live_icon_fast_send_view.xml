<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/gift_fast_sent_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.wali.live.watchsdk.fastsend.view.CircleProgressBar
        android:id="@+id/live_icon_fast_send_circle"
        android:layout_width="@dimen/view_dimen_101"
        android:layout_height="@dimen/view_dimen_101"
        android:layout_gravity="center" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/live_icon_fast_send_img"
        android:layout_width="@dimen/view_dimen_110"
        android:layout_height="@dimen/view_dimen_110"
        android:layout_gravity="center"
        android:background="@drawable/live_icon_flower_bg_normal" />

</FrameLayout>