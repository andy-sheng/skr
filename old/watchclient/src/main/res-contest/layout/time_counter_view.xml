<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/view_dimen_170"
    android:layout_height="@dimen/view_dimen_170">

    <ImageView
        android:id="@+id/time_end_iv"
        android:layout_width="@dimen/view_dimen_142"
        android:layout_height="@dimen/view_dimen_142"
        android:layout_centerInParent="true"
        android:src="@drawable/youle_live_answer_icon_time" />

    <TextView
        android:id="@+id/count_down_left_time_tv"
        android:layout_width="@dimen/view_dimen_142"
        android:layout_height="@dimen/view_dimen_142"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size_80"
        android:textStyle="bold"
        android:visibility="gone" />

    <com.wali.live.watchsdk.view.TimeCounterCircle
        android:id="@+id/count_down_anim_circle"
        android:layout_width="@dimen/view_dimen_170"
        android:layout_height="@dimen/view_dimen_170"
        android:layout_centerInParent="true" />
</merge>